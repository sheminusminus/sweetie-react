{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["@babel/plugin-syntax-decorators", { "decoratorsBeforeExport":  false }],
    ["@babel/plugin-proposal-class-properties", { "loose":  true }],
    ["@babel/plugin-proposal-private-methods", { "loose":  true }]
  ]
}
